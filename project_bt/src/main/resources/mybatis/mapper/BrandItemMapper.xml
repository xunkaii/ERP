<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.erp.dao.BrandItemDao">
   <select id="schBri" parameterType="bi" resultType="com.web.erp.vo.BrandItem">
       SELECT ITEM_NUM, ITEM_NAME, REF_NUM
       FROM BRAND_ITEM
       WHERE REF_NUM = #{ref_num}
   </select>
</mapper>
 