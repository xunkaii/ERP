		<typeAlias alias="schprd" type="com.web.erp.vo.SchPrd"/>
		<typeAlias alias="delHeadPrd" type="com.web.erp.vo.HeadOfficeProduct"/>
		<typeAlias alias="bi" type="com.web.erp.vo.BrandItem"/>
		<typeAlias alias="sales" type="com.web.erp.vo.Sales"/>
		<typeAlias alias="apv" type="com.web.erp.vo.Approval"/>
		<typeAlias alias="hop" type="com.web.erp.vo.HeadOfficeProduct"/>
		<typeAlias alias="salesDetail" type="com.web.erp.vo.SalesDetail"/>
		<typeAlias alias="schApv" type="com.web.erp.vo.SchApv"/>
		<typeAlias alias="salesByDay" type="com.web.erp.vo.SalesByDay"/>
