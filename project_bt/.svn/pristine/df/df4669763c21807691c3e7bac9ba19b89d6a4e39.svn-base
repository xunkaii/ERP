		<typeAlias alias="employee" type="com.web.erp.vo.Employee"/>
		<typeAlias alias="prm" type="com.web.erp.vo.Prm"/>
		<typeAlias alias="storeV" type="com.web.erp.vo.StoreVO"/>
		<typeAlias alias="schStore" type="com.web.erp.vo.SchStore"/>
		<typeAlias alias="schproperties" type="com.web.erp.vo.ProductProperties"/>
